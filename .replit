language = "php"

run = [
    "php", "artisan", "serve",
    "--host", "0.0.0.0",
    "--port", "8000"
]

entrypoint = "routes/web.php"

hidden = [
    "artisan",

    ".editorconfig",
    ".gitattributes",
    ".gitignore",
    ".styleci.yml",

    "composer.lock",
    "vendor",
    "phpunit.xml"
]

[packager]
language = "php"
